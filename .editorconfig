root = true

[*]
end_of_line = LF

[*.js]
indent_style = space
indent_size = 4
trim_trailing_whitespace = true
insert_final_newline = true

[*.json]
indent_style = space
indent_size = 4
trim_trailing_whitespace = true
insert_final_newline = true

[*.yml]
indent_style = space
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
